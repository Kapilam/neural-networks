    "import math\n",
    "x1 = 1\n",
    "x2 = 1\n",
    "# calculate score\n",
    "score = 4*x1 + 5*x2-9\n",
    "print(score)\n",
    "\n",
    "# sgmoid function\n",
    "def sigmoide(x):\n",
    "    y = 1/(1+math.exp(-x) )\n",
    "    return y\n",
    "\n",
    "# apply sigmoide function to calculate probability\n",
    "\n",
    "print(sigmoide(score))\n"
